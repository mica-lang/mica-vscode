{
    "name": "mica",
    "displayName": "Mica",
    "description": "Syntax highlighting for the Mica programming language",
    "version": "0.0.1",
    "repository": "https://github.com/mica-lang/mica-vscode",
    "engines": {
        "vscode": "^1.65.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "mica",
            "aliases": ["Mica", "mica"],
            "extensions": [".mi"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "mica",
            "scopeName": "source.mica",
            "path": "./syntaxes/mica.tmLanguage.json"
        }]
    }
}
